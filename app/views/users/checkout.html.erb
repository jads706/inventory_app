<!--Checkout Table -->
        <h3>Checkout </h3>
        <table class="table">
      <thead>
        <tr>
          <th scope="col">Name</th>
          <th scope="col">VSU ID</th>
          <th scope="col">Model</th>
          <th scope="col">Brand</th>
          <th scope="col">Condition</th>
          <!--<th scope="col">Location</th>-->
          <!--<th scope="col">Edit</th>-->
          <th scope="col">Checkout</th>
        </tr>
      </thead>
      <tbody>
        <% Unit.all.each do |unit| %>
          <% if unit.location == "storage" %>
          <tr>
            
            <td><%= unit.name_id %></td>
            <td><%= unit.vsu_id %></td>
            <td><%= unit.product_type.model %></td>
            <td><%= unit.product_type.brand %></td>
            <td><%= unit.condition %></td>
            <!--<td><%#= link_to 'Edit', "#" %></td>-->
            <% if current_user.admin? %>
            <td><%= form_with(model: Unit, local: true) do |f| %>
                <%= f.select :location, User.all.map{|c| [c.name,c.id]}, :prompt => "Select One"%>
            <%#= select_tag(unit.location, options_from_collection_for_select(User.all, :id, :name), prompt: "Select One")%></td>
            <td><%= f.submit "Checkout", class: "btn btn-primary" %>
                <% end %>
              <%#= submit_tag( "checkout", data: { confirm: "Are you sure?" }) %></td>
            <% else %>
              
              <td>
                <% if unit.requestor == "N/A" %>
                  <%= link_to 'Checkout', checkout_unit_path(unit), method: :patch %>
                <% else %>
                  Pending for Request
                <%end%>
              </td>
            <%end%>
          </tr>
          <% end %>
        <% end %>
      </tbody>
    </table>