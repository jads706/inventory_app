<!--User Table -->
        <h3>Users </h3>
        <table class="table">
      <thead>
        <tr>
          <th scope="col">Name</th>
          <th scope="col">Email</th>
          <!--<th scope="col">User Status</th>-->
          <th scope="col">Accept</th>
          <th scope="col">Reject</th>
          <!--<th scope="col">Location</th>-->
          <!--<th scope="col">Edit</th>-->
        </tr>
      </thead>
      <tbody>
        <%= will_paginate @users%>
        <% User.all.each do |user| %>
          <tr>
            <% if !user.activated %>
            <td><%= user.name %></td>
            <td><%= user.email %></td>
            <td><%= link_to "Activate", edit_account_activation_url(user.activation_token,
                email: user.email, id: user.id) %></td>
            <td>
              <%= link_to 'Reject', user, method: :delete, data: { confirm: "You sure?" } %>
            </td>
            <% end %>
            
          </tr>
      </tbody>
       <% end %>
    </table>
    <%= will_paginate @users%>